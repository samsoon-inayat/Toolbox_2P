    M = max(thisRaster(:));
    xdata = [0 75 150]; ydata = [1 10];
    imagesc(xdata,ydata,thisRaster,[m M]);
    set(gca,'Ydir','normal');
    % axis off;
    box off;
    if cc > 1
%     set(gca,'ytick',[1 size(thisRaster,1)],'TickDir','out');
    
    xlabel('Distance (cm)'); ylabel('Trial #');
    format_axes_b(gca)
    colormap jet;
