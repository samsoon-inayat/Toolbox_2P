[within,dvn,xlabels,awithinD] = make_within_table({'TI','CT','Cond'},[2,3,3]);
dataT = make_between_table({varC},dvn);
ra = RMA(dataT,within,{0.05,{'bonferroni'}});
ra.ranova
print_for_manuscript(ra)
